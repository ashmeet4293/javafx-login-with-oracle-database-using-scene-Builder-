            <Font size="20.0" />
         </font>
      </Label>
      <PasswordField fx:id="txtPassword" layoutX="210.0" layoutY="145.0" promptText="password" />
      <Button id="txtSignup" layoutX="286.0" layoutY="197.0" mnemonicParsing="false" onAction="#signUpAction" text="Sign up" />
